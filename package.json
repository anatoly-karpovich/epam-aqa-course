{
  "name": "my-new-project",
  "type": "module",
  "devDependencies": {
    "@types/ajv": "^1.0.0",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.3.11",
    "@types/dotenv": "^8.2.0",
    "@types/form-data": "^2.5.0",
    "@types/jasmine": "^5.1.1",
    "@types/lodash": "^4.14.199",
    "@types/mocha": "^10.0.6",
    "@types/rimraf": "^4.0.5",
    "@types/superagent": "^4.1.22",
    "@types/winston": "^2.4.4",
    "@wdio/allure-reporter": "^8.17.0",
    "@wdio/cli": "^8.17.0",
    "@wdio/cucumber-framework": "^8.20.3",
    "@wdio/jasmine-framework": "^8.20.0",
    "@wdio/local-runner": "^8.17.0",
    "@wdio/mocha-framework": "^8.17.0",
    "@wdio/spec-reporter": "^8.17.0",
    "allure-mocha": "^2.9.2",
    "cross-env": "^7.0.3",
    "mocha": "^10.2.0",
    "mocha-multi-reporters": "^1.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "test": "npm run build && wdio run ./src/config/wdio.conf.ts",
    "test:ui": "npm run test -- --suite ui",
    "test:api": "npm run test -- --suite api",
    "test:cucumber-run": "cross-env TEST_RUNNER=cucumber npm run test -- --cucumberOpts.tags='@run'",
    "mocha-api": "rimraf src/report/allure-results && npm run build && mocha --config .mocharc.json",
    "test:mocha-api": "cross-env FRAMEWORK=mocha npm run mocha-api",
    "build": "rimraf dist && npx tsc",
    "report": "allure generate src/report/allure-results --clean -o src/report/allure-report",
    "report-open": "npm run report && allure open src/report/allure-report"
  },
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "ajv": "^8.12.0",
    "allure-commandline": "^2.24.1",
    "axios": "^1.6.2",
    "chai": "^4.3.10",
    "dotenv": "^16.3.1",
    "form-data": "^4.0.0",
    "lodash": "^4.17.21",
    "rimraf": "^5.0.5",
    "superagent": "^8.1.2",
    "winston": "^3.11.0"
  }
}
